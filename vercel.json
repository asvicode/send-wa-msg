{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["components/**"] }
    },
    {
      "src": "view/**",
      "use": "@vercel/static",
      "config": { "includeFiles": ["view/**"] }
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/",
      "dest": "view/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "view/$1"
    }
  ]
}